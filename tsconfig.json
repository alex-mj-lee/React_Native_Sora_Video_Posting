{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "app/(auth)/sing-in.jsx",
    "app/(auth)/sign-up.jsx",
    "app/(auth)/_layout.jsx",
    "app/(tabs)/_layout.jsx",
    "app/(tabs)/bookmark.jsx",
    "app/(tabs)/create.jsx",
    "app/(tabs)/home.jsx",
    "app/(tabs)/profile.jsx",
    "app/search/[query].jsx",
    "app/_layout.jsx",
    "app/index.jsx",
    "app/profile.jsx",
    "constants/Colors.js",
    "constants/icons.js",
    "constants/index.js",
    "constants/images.js",
    "constants/Colors.js"
  ]
}
